#include <iostream>
using namespace std;

int main() {
   
   int arr[2][2]={{1,2},{4,5}}; //row  //column  //1st row and 2nd row
   
   for(int i=0;i<2;i++){ //row
       for(int j=0;j<2;j++){ //column
           cout<<arr[i][j]<<" "; //arr[0][0] 0,1  1,0  1,1
       }
      cout<<"\n";
   }
   
   return 0;
}
