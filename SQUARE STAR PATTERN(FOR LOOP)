#include<iostream>
using namespace std;
int main()
{
    cin>>num;
    for(int i=0;i<=num;i++){
        for(int j=0;j<=num;j++){
            cout<<"*";
        }
        cout<<"\n";
    }
}

//without using outer block statements

#include<iostream>
using namespace std;
int main()
{
    cin>>num;
    for(int i=0;i<=num;i++,cout<<"\n"){
        for(int j=0;j<=4;j++){
            cout<<"*";
        }
    }
}
