#include<iostream>
using namespace std;
int main()
{
    for(int i=0;i<=4;i++){
        for(int j=0;j<=4;j++){
            cout<<"*";
        }
        cout<<"\n";
    }
}

//without using outer block statements

#include<iostream>
using namespace std;
int main()
{
    for(int i=0;i<=4;i++,cout<<"\n"){
        for(int j=0;j<=4;j++){
            cout<<"*";
        }
        
    }
}
