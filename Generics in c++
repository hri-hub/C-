#include <iostream>
using namespace std;

template <typename T>
T add(T a, T b) {
    return a + b;
}

int main() {
    cout << add(3, 4) << endl;        // Calls add<int>
    cout << add(3.5, 4.5) << endl;    // Calls add<double>
    cout << add('A', 'B') << endl;    // Calls add<char>
    return 0;
}
