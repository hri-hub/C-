#include <iostream>
using namespace std;
int main() {
    
    int arr[2][2][2]={1,2,3,4,5,6,7,8};
    
    for(int i=0;i<2;i++){ //No of Matrix
        for(int j=0;j<2;j++){ //No of Rows
            for(int k=0;k<2;k++){ //No of Columns
                cout<<arr[i][j][k]<<" ";
            }
            cout<<"\n";
        }
        cout<<"\n";
    }
    
    return 0;
}
