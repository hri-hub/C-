//dynamic
#include <iostream>
using namespace std;
int main() 
{
    int n;
    cin>>n;
    int arr[5]={2,5,4,34,6};
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    for(int i=n-1;i>=0;i--){
        cout<<arr[i]<<" ";
    }
}

\\static
#include <iostream>
using namespace std;
int main() 
{
    int arr[5]={2,5,4,34,6};
    
    for(int i=sizeof(arr)/sizeof(arr[0])-1;i>=0;i--){
        cout<<arr[i]<<" ";
    }
}

