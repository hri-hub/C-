//Find the Sum of the Array

#include <iostream>
using namespace std;
int main() {
   
   int arr[]={1,2,3,4,5,6};

   for(int i=0;i<sizeof(arr)/sizeof(arr[0]);i++){ 
       if(arr[i]%2==0){
           cout<<"Even="<<arr[i]<<"\n";
       }
       else{
           cout<<"Odd="<<arr[i]<<"\n";
       }
   }
    return 0;
}
